{
  "name": "iods/module-baseui",
  "description": "An extension of the Iods_Base package, improving FE functionality in Magento 2.",
  "type": "magento2-module",
  "version": "000.1.1",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Rye Miller",
      "email": "rye@drkstr.dev",
      "homepage": "https://ryemiller.io"
    }
  ],
  "minimum-stability": "dev",
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "require": {
    "php": ">=8.1",
    "iods/module-base": "*",
    "magento/framework": ">=103.0.5-p1",
    "symfony/console": "*"
  },
  "autoload": {
    "files": ["registration.php"],
    "psr-4": {
      "Iods\\BaseUi\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": true
    }
  }
}
