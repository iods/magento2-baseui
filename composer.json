{
  "name": "iods/module-baseui",
  "description": "An extension of the base module improving FE functionality of Magento 2.",
  "type": "magento2-module",
  "version": "000.1.0",
  "license": [
    "MIT"
  ],
  "authors": [
    {
      "name": "Rye Miller",
      "email": "rye@drkstr.dev",
      "homepage": "https://ryemiller.io",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  },
  "require": {
    "php": ">=8",
    "magento/framework": ">=104",
    "symfony/console": "*"
  },
  "require-dev": {
    "phpstan/extension-installer": "*",
    "phpstan/phpstan": "*",
    "phpstan/phpstan-phpunit": "*",
    "phpunit/phpunit": "*",
    "composer/composer": "*@dev"
  },
  "autoload": {
    "files": ["registration.php"],
    "psr-4": {
      "Iods\\BaseUi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Iods\\BaseUi\\Tests\\Unit\\": "tests/unit"
    }
  },
  "scripts": {
    "test": [
      "@phpstan",
      "@phpunit"
    ],
    "phpstan": "phpstan analyse",
    "phpunit": "phpunit --no-coverage"
  }
}
